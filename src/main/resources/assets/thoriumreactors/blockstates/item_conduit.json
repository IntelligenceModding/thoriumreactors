{
  "multipart": [
    {   "apply": { "model": "thoriumreactors:block/conduit_core" }},
    {   "when": { "north": "enabled" },
      "apply": { "model": "thoriumreactors:block/conduit_part", "uvlock": false }
    },
    {   "when": { "east": "enabled" },
      "apply": { "model": "thoriumreactors:block/conduit_part", "y": 90, "uvlock": false }
    },
    {   "when": { "south": "enabled" },
      "apply": { "model": "thoriumreactors:block/conduit_part", "y": 180, "uvlock": false }
    },
    {   "when": { "west": "enabled" },
      "apply": { "model": "thoriumreactors:block/conduit_part", "y": 270, "uvlock": false }
    },
    {   "when": { "up": "enabled" },
      "apply": { "model": "thoriumreactors:block/conduit_part", "x": 270, "uvlock": false }
    },
    {   "when": { "down": "enabled" },
      "apply": { "model": "thoriumreactors:block/conduit_part", "x": 90, "uvlock": false }
    },
    {   "when": { "north": "output" },
      "apply": { "model": "thoriumreactors:block/conduit_part_extract", "uvlock": false }
    },
    {   "when": { "east": "output" },
      "apply": { "model": "thoriumreactors:block/conduit_part_extract", "y": 90, "uvlock": false }
    },
    {   "when": { "south": "output" },
      "apply": { "model": "thoriumreactors:block/conduit_part_extract", "y": 180, "uvlock": false }
    },
    {   "when": { "west": "output" },
      "apply": { "model": "thoriumreactors:block/conduit_part_extract", "y": 270, "uvlock": false }
    },
    {   "when": { "up": "output" },
      "apply": { "model": "thoriumreactors:block/conduit_part_extract", "x": 270, "uvlock": false }
    },
    {   "when": { "down": "output" },
      "apply": { "model": "thoriumreactors:block/conduit_part_extract", "x": 90, "uvlock": false }
    }
  ]
}